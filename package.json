{
  "name": "svelte-webpack-boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --mode development",
    "build": "node ./node_modules/webpack/bin/webpack.js --mode production",
    "lint:js": "node ./node_modules/eslint/bin/eslint.js --ext .js,.svelte ./src",
    "lint:style": "node ./node_modules/stylelint/bin/stylelint.js --config ./.stylelintrc.json ./src/**/*.pcss",
    "lint": "npm run lint:js && npm run lint:style",
    "prettier": "node ./node_modules/pretty-quick/bin/pretty-quick.js --staged",
    "prettier:all": "node ./node_modules/prettier/bin-prettier.js ./src/**/*.svelte --write",
    "docker:setup": "",
    "docker:build": "docker build --tag svelte-webpack-boilerplate:latest --no-cache .",
    "docker:start": "echo \"Starting the container as a daemon; run 'npm run docker:logs' to see the logs\" && npm run docker:clean && npm run docker:setup && docker run -p 8080:80 --name=svelte-webpack-boilerplate -d svelte-webpack-boilerplate:latest",
    "docker:clean": "docker rm -f svelte-webpack-boilerplate || true",
    "docker:logs": "docker logs svelte-webpack-boilerplate"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:svelte-construction/svelte-webpack-boilerplate.git"
  },
  "author": "Pavel Kondratenko <iam@kondratenko.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svelte-construction/svelte-webpack-boilerplate/issues"
  },
  "pre-commit": [
    "prettier",
    "lint"
  ],
  "homepage": "https://github.com/svelte-construction/svelte-webpack-boilerplate#readme",
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-destructuring": "7.6.0",
    "@babel/preset-env": "7.6.3",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-transform-es2015-spread": "6.22.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.4.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-svelte3": "2.7.3",
    "file-loader": "3.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "markdown-loader": "5.1.0",
    "mini-css-extract-plugin": "0.7.0",
    "postcss": "7.0.18",
    "postcss-automath": "1.0.1",
    "postcss-color-mod-function": "3.0.3",
    "postcss-each": "0.10.0",
    "postcss-for": "2.1.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-mixins": "6.2.2",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.6.0",
    "postcss-simple-vars": "5.0.2",
    "pre-commit": "1.2.2",
    "prettier-plugin-svelte": "0.7.0",
    "pretty-quick": "2.0.0",
    "style-loader": "0.23.1",
    "stylelint": "10.1.0",
    "stylelint-config-prettier": "5.2.0",
    "stylelint-order": "3.0.0",
    "svelte": "~3.12.1",
    "svelte-loader": "2.13.4",
    "svelte-preprocess": "2.15.0",
    "svgo": "1.3.0",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.3.3",
    "webpack-dev-server": "3.5.1"
  },
  "dependencies": {
    "core-js": "3.3.5",
    "regenerator-runtime": "0.13.3",
    "svelte-routing": "1.2.0"
  }
}
